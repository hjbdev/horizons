  "author": "Harry (hjbdev) <30434619+HJBDev@users.noreply.github.com>",
  "license": "MIT",
  "devDependencies": {
    "cross-env": "^7.0.2",
    "laravel-mix": "^5.0.4",
    "resolve-url-loader": "^3.1.0",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
